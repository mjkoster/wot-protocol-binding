{
  "title": "Here are the schema examples for the link-format TD"
},
{
  "id": "tempschema",
  "properties": {
    "temperature": {
      "semtype": "sch:temperature",
      "type": "number"
    }
  }
},
{
  "id": "alertschema",
  "properties": {
    "alert.type": {
      "type": "string",
      "value": "Low Temperature"
    },
    "temperature": {
      "semtype": "sch:temperature",
      "type": "number"
    }
  }

{
	"@context": [
		"http://w3c.github.io/wot/w3c-wot-td-context.jsonld",
		{ "sch": "http://iot.schema.example.org/schemas#" }
	],
	"@type": ["Thing", "sch:actuator"],
	"name": "OCF Temperature Example",
	"base": "coap://example.com:5683/",
	"interactions": [
		{
			"@type": ["Property","sch:temperature"],
			"name": "temperature",
			"outputData": {
			  "properties": { 
			    "temperature": { 
			      "type": "number"
			    }
			  }
			"links": [
			  {
				"href" : "temperature", 
				"mediaType": "application/vnd.ocf+cbor",
				"method": ["ocf.retrieve"],
				"rt": ["oic.r.temperature"],
				"if": ["oic.if.a"] 
			  }
			]
		},
		{
			"@type": ["Action","sch:change"],
			"name": "Change Temperature",
			"inputData": {
			  "properties": { 
			    "temperature": { 
			      "type": "number",
			      "writable": true
			},
			"links": [
			  {
				"href" : "temperature", 
				"mediaType": "application/vnd.ocf+cbor",
				"method": ["ocf.update"],
				"rt": ["oic.r.temperature"],
				"if": ["oic.if.a"] 
			  }
			]									
		},
		{
			"@type": ["Event","sch:alert"],
			"name": "Low Temperature",
			"outputData": {"valueType": { "type": "string" }},
			"links": [
			  {
                "href" : "events",
                "mediaType": "application/vnd.ocf+cbor",
                "method": "ocf.observe",
				"rt": ["oic.r.eventsource"],
				"if": ["oic.if.a"] 
              }
            ]	
		}
	]
}
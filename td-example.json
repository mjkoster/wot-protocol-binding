{
	"@context": [
		"http://w3c.github.io/wot/w3c-wot-td-context.jsonld",
	    "http://iot.schema.example.org/schemas/temperature.jsonld",
	    "http://iot.schema.exam[ple.org/schemas/actiontypes.jsonld"
	],
	"@type": ["thing", "sch:actuator"],
	"name": "OCF Temperature Example",
	"base": "coap://example.com:5683/",
	"interactions": [
		{
			"@type": ["property","sch.temperature"],
			"name": "Storage Temperature",
			"outputData": {"valueType": { "type": "number" }},
			"links": [
			  {
				"href" : "temperature", 
				"mediaType": "application/vnd.ocf+cbor",
				"method": ["ocf.retrieve"],
				"rt": ["oic.r.temperature"],
				"if": ["oic.if.a"],
				"schema": {
			      "properties": { 
			        "temperature": { 
			          "type": "number"
			        } 
			      }
			    }
			  }
			]
		},
		{
			"@type": ["action","sch;change"],
			"name": "Change Storage Temperature",
			"inputData": {"valueType": { "type": "number" }},
			"links": [
			  {
				"href" : "temperature", 
				"mediaType": "application/vnd.ocf+cbor",
				"method": ["ocf.update"],
				"rt": ["oic.r.temperature"],
				"if": ["oic.if.a"], 
				"schema": {
			      "properties": { 
			        "temperature": { 
			          "type": "number"
			        } 
			      }
			    }
			  }
			]									
		},
		{
		    "@type": ["event","sch:alert"],
			"name": "Low Storage Temperature Alert",
			"outputData": {"valueType": { "type": "string" }},
			"links": [
			  {
                "href" : "alerts/lowtemperature",
                "mediaType": "application/vnd.ocf+cbor",
                "method": "ocf.observe",
				"rt": ["oic.r.eventsource"],
				"if": ["oic.if.r"] 
              }
            ]	
		}
	]
}